<div style="height: 655px;">
  <div class="container mt-3 mb-3">
    <div class="row">
      <div class="col"></div>
      <div class="col">
        <span class="h1">会員一覧</span>
      </div>
      <div class="col">
        <%= form_with(url: admin_members_path, method: :get, data: {remote: true}) do |f| %>
          <span class="font-weight-bold mr-3">退会/有効</span><br>
          <%= f.select(:is_deleted, [['有効', :false], ['退会', :true]], {prompt: '選択'}, class: "form form-control-sm", style:"width: 100px;") %>
          <%= f.submit '表示', class:"btn btn-primary ml-3" %>
        <% end %>
      </div>
    </div>
  </div>

  <div class="container" id="member-list">
    
            <%= render 'admin/members/index', members: @members %>
          
  </div>
</div>